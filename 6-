// 6.	Comprueba si un ArrayList de números enteros está vacío.
import java.util.ArrayList;
public class Main {
    public static void main(String[] args) {
        ArrayList<Integer> listaEnteros = new ArrayList<>();
        ArrayList<Integer> listaEnteros2 = new ArrayList<>();
        listaEnteros.add(1);
        if (listaEnteros.isEmpty()){System.out.println("La lista 1 está vacía");}
        if (listaEnteros2.isEmpty()){System.out.println("La lista 2 está vacía");}
    }
}
