// 4.	Encuentra la longitud (nÃºmero de elementos) de un ArrayList de caracteres.
import java.util.ArrayList;
public class Main {
    public static void main(String[] args) {
        ArrayList<Character> listaCaracteres = new ArrayList<>();
        listaCaracteres.add('a');
        listaCaracteres.add('$');
        listaCaracteres.add('/');
        listaCaracteres.add('P');
        listaCaracteres.add('{');
        System.out.println(listaCaracteres.size());
    }
}
