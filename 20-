// 20.	Encuentra el elemento en un ArrayList de cadenas que tiene la longitud m치s larga.
import java.util.ArrayList;
public class Main {
    public static void main(String[] args) {
        String elementoMayorLong = "";
        ArrayList<String> listaNombres = new ArrayList<>();
        listaNombres.add("Java");
        listaNombres.add("Programaci칩n");
        listaNombres.add("C칩digo");
        for (int i = 0; i < listaNombres.size(); i++){
            if (listaNombres.get(i).length() > elementoMayorLong.length()){
                elementoMayorLong = listaNombres.get(i);
            }
        }
        System.out.println(listaNombres);
        System.out.println("El elemento con m치s longitud de la lista es: "+elementoMayorLong);
    }
}
